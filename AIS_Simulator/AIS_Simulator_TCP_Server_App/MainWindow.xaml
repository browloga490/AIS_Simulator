<Window x:Class="AIS_Simulator_TCP_Server_App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AIS_Simulator_TCP_Server_App"
        xmlns:uc="clr-namespace:AIS_Simulator_TCP_Server_App.UserControls"
        xmlns:local1="clr-namespace:AIS_Simulator_TCP_Server_App.Resource_Dictionaries"
        mc:Ignorable="d"
        Title="AIS Simulator TCP Server" Height="496.5" Width="634">
    <Window.Resources>
        <local1:ViewModel x:Key="viewModel"/>
    </Window.Resources>
    <Grid x:Name="Server">
        <uc:UserControlMainWindow DataContext="{Binding Source={StaticResource viewModel}}"/>
    </Grid>
</Window>
